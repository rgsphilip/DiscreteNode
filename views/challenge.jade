extends layout
block content

      .container.learnText
        .row
          .col-md-8
            h3.learnTitle Challenge Title
            p.learnQuestion Question
            .checkAns
              .learnAnswer
              button.btn.btn-primary.checkButton(type='button') Check Answer
              .feedback
          .col-md-4
            h3.hints 
            p.hintContent 
      .container
        .row.navButtons
          .col-md-6
            button.btn.btn-lg.btn-primary.prevButton(type='button') Previous
            
          .col-md-6
            button.btn.btn-lg.btn-primary.nextQuestion(type='button', disabled='disabled') Next
       
       
       if completed
         br
         h1.longPara Great job! You have finished all the set material. Go check out your 
            a#darkLink(href='profile') progress
            |.        
            
       footer
         p Created by Rachel Philip
       //Included a script here that uses the value of the user's last answered question to set that on the window
      script(type="text/javascript").
        window.lastQAnswered = "!{lastQAnswered}";
       
      script(src='javascripts/functions.js')
      script(src='setChallenge/subsets1.js')
      script(src='setChallenge/subsets2.js')
      script(src='setChallenge/subsets3.js')
      script(src='setChallenge/setOperators1.js')
      script(src='setChallenge/setOperators2.js')
      script(src='setChallenge/setOperators3.js')
      script(src='setChallenge/setOperators4.js')
      script(src='setChallenge/powerSets1.js')
      script(src='setChallenge/countingPrinciples1.js')
      script(src='setChallenge/countingPrinciples2.js')
      script(src='javascripts/challengeSets.js')
